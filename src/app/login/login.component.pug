h1 Heartbeat
div.card 
    h2 Sign in
    
    input.wide([(ngModel)]="username", placeholder="Username")
    input.wide([(ngModel)]="password", placeholder="Password", type="password")
    
    small
        p#error {{ err }}

.actions
    button((click)="goToRegister()") New account
    button((click)="signIn()") Sign in
